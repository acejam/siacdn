#!/usr/bin/env bash

set -e

PROJECT_NAME="gradientzoo-1233"
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

cd "$DIR/../../backend"

kubectl --namespace=sia exec -it $(kubectl --namespace=sia get pods -l app=siacdn-backend -o jsonpath="{.items[0].metadata.name}") -- sh -c 'cat /siacdn/data/database.json' > database.json
